	function changeQuantity ($el, $incr, $stock) {
		$el = jQuery($el);
		var $item = $el.closest('.mwCart-item');
		var $qty = $item.find('.mwCart-quantity');

		if ( $qty.length >= 1 ) {
			if ($incr && $stock <= 0) {
				$qty.val( Math.min( $qty.val() * 1 + 1) );
			} else if ($incr) {
				$qty.val( Math.min( $qty.val() * 1 + 1, $stock) );
			} else {
				$qty.val( Math.max( $qty.val() * 1 - 1, 1) );
			}
		}
	}
